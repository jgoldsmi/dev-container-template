{
  "name": "Devcontainer Template",
  "build": {
    "dockerfile": "Containerfile",
    "context": ".",
    "args": {
      "GITHUB_USER": "jgoldsmi"
    }
  },
  "features": {},
  "remoteUser": "devcontainer",
  "mounts": [
    "source=${localEnv:HOME}/.ssh,target=/home/devcontainer/.ssh,type=bind,consistency=cached"
  ],
  "remoteEnv": {
    "SHELL": "/usr/bin/fish"
  }
}
